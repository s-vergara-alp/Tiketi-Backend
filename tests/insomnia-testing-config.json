        {
          "id": "req_auth_me",
          "name": "Get Profile",
          "type": "request"
        }
      ],
      "beforeRequestScript": "// Set up test data\nconst timestamp = Date.now();\ninsomnia.environment.set('test_username', `testuser_${timestamp}`);\ninsomnia.environment.set('test_email', `test_${timestamp}@example.com`);",
      "afterResponseScript": "// Extract auth token from login response\nif (insomnia.request.name === 'Login' && insomnia.response.status === 200) {\n  const responseData = JSON.parse(insomnia.response.body);\n  if (responseData.token) {\n    insomnia.environment.set('auth_token', responseData.token);\n    console.log('Auth token set:', responseData.token.substring(0, 20) + '...');\n  }\n}",
      "_type": "unit_test_suite"
    },
